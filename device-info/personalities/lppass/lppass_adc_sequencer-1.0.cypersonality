      <ParamString id='sar_gpiochan_$idx_enabled' name='sar_gpiochan_$idx_enabled' group='Internal' visible='false' desc='' editable='false' default='`${getExposedMember(resource_sar, "sar_gpiochan_$idx_enabled")}`'/>
    </Repeat>

    <Repeat count='16'>
      <ParamString id='sar_muxchan_$idx_enabled' name='sar_muxchan_$idx_enabled' group='Internal' visible='false' desc='' editable='false' default='`${getExposedMember(resource_sar, "sar_muxchan_$idx_enabled")}`'/>
      <ParamString id='muxchan_$idx_pseudoDiffMode' name='muxchan_$idx_pseudoDiffMode' group='Internal' visible='false' desc='' editable='false' default='`${sar_muxchan_$idx_enabled ? getExposedMember("pass[" . inst_pass . "].sar[" . inst_sar . "].muxchan[" . $idx . "]",  "sar_mux_pseudoDiffMode") : false}`'/>
    </Repeat>

<!-- LPPASS internal parameters -->
    <ParamString id='rootGroupName' name='rootGroupName' group='Internal' visible='`${debug}`' desc='' editable='false'
                 default='Sequencer Configuration'/>

    <ParamChoice id='channelMux'
                 name='Channel'
                 group='`${rootGroupName}`'
                 visible='`${(channelMuxEnable) &amp;&amp; (operatingMode eq "LowPower")}`'
                 editable='true'
                 desc='List of configured Mux channels'
                                   default='CY_AUTANALOG_SAR_CHAN_CFG_MUX0'>
      <Entry name='Mux channel $idx' value='CY_AUTANALOG_SAR_CHAN_CFG_MUX$idx' visible='`${sar_muxchan_$idx_enabled}`' repeatCount='16'/>
    </ParamChoice>

    <ParamChoice id='channelGpio'
                 name='Channel'
                 group='`${rootGroupName}`'
                 visible='`${(channelGpioEnable || channelMuxEnable) &amp;&amp; (operatingMode eq "HighSpeed") &amp;&amp; (!simultaneousSampling)}`'
                 editable='true'
                 desc='List of configured Mux and GPIO channels'
                                        default='CY_AUTANALOG_SAR_CHAN_MASK_GPIO_DISABLED'>
      <Entry name='Disabled'              value='CY_AUTANALOG_SAR_CHAN_MASK_GPIO_DISABLED' visible='true'/>
      <Entry name='MUX channel $idx'      value='CY_AUTANALOG_SAR_CHAN_CFG_MUX$idx'   visible='`${sar_muxchan_$idx_enabled}`'  repeatCount='16'/>
      <Entry name='GPIO channel $idx'     value='CY_AUTANALOG_SAR_CHAN_MASK_GPIO$idx' visible='`${sar_gpiochan_$idx_enabled}`' repeatCount='8'/>
    </ParamChoice>

    <Repeat count='2'>
       <ParamBool id='channelMux$idx'
               name='Mux Channel $idx'
               group='`${rootGroupName}`'
               visible='`${(channelMuxEnable) &amp;&amp; (operatingMode eq "HighSpeed") &amp;&amp; (simultaneousSampling)}`'
               editable='true'
               desc='Enable Mux Channel $idx in Scan'
               default='false'/>
    </Repeat>

    <Repeat count='8'>
        <ParamBool id='channelGpio$idx'
               name='GPIO Channel $idx'
               group='`${rootGroupName}`'
               visible='`${(channelGpioEnable) &amp;&amp; (operatingMode eq "HighSpeed") &amp;&amp; (simultaneousSampling) &amp;&amp; (sar_gpiochan_$idx_enabled)}`'
               editable='true'
               desc='Enable GPIO Channel $idx in Scan'
               default='false'/>
    </Repeat>

    <Repeat count='2'>
       <ParamChoice id='sar_mux_$idxChoice'
                 name='Mux Channel $idx'
                 group='`${rootGroupName}`'
                 visible='`${(channelMuxEnable) &amp;&amp; (operatingMode eq "HighSpeed") &amp;&amp; (simultaneousSampling) &amp;&amp; (channelMux$idx)}`'
                 editable='true'
                 desc='List of configured Mux channels'
                                      default='CY_AUTANALOG_SAR_CHAN_CFG_MUX0'>
         <Entry name='Mux channel $idx' value='CY_AUTANALOG_SAR_CHAN_CFG_MUX$idx' visible='`${sar_muxchan_$idx_enabled}`' repeatCount='16'/>
       </ParamChoice>
    </Repeat>

    <ParamBool id='sampleTimeEnable'
               name='Sample Time'
               group='`${rootGroupName}`'
               visible='`${(channelMuxEnable || channelGpioEnable)}`'
               editable='`${!(operatingMode eq "HighSpeed" ? hsAutosetConversionRate : lpAutosetConversionRate)}`'
               desc='Enable a Global Sampling time for this scan'
               default='`${!(operatingMode eq "HighSpeed" ? hsAutosetConversionRate : lpAutosetConversionRate)}`'/>

    <ParamChoice id='sampleTimeSelect'
                 name='Sample Time Select'
                 group='`${rootGroupName}`'
                 visible='`${(channelMuxEnable || channelGpioEnable) &amp;&amp; (sampleTimeEnable)}`'
                 editable='true'
                 desc='Sample time clocks, uS'
                                          default='CY_AUTANALOG_SAR_SAMPLE_TIME0'>
      <Entry name='Global Sample Time $idx' value='CY_AUTANALOG_SAR_SAMPLE_TIME$idx' visible='true' repeatCount='4'/>
    </ParamChoice>

    <ParamBool id='accumulationEnable'
               name='Accumulation Enable'
               group='`${rootGroupName}`'
               visible='`${(channelMuxEnable || channelGpioEnable) &amp;&amp; (accMode ne "CY_AUTANALOG_SAR_ACC_DISABLED")}`'
               editable='`${!(operatingMode eq "HighSpeed" ? hsAutosetConversionRate : lpAutosetConversionRate)}`'
               desc='Enable accumulation over multiple samples'
               default='`${!(operatingMode eq "HighSpeed" ? hsAutosetConversionRate : lpAutosetConversionRate)}`'/>

    <ParamChoice id='accCount'
                 name='Accumulation Count'
                 group='`${rootGroupName}`'
                 visible='`${(channelMuxEnable || channelGpioEnable) &amp;&amp; (accumulationEnable) &amp;&amp; (accMode ne "CY_AUTANALOG_SAR_ACC_DISABLED")}`'
                 editable='true'
                 desc='Number of samples accumulated '
                    default='CY_AUTANALOG_SAR_ACC_CNT2'>
      <Entry name='2'   value='CY_AUTANALOG_SAR_ACC_CNT2'   visible='true'/>
      <Entry name='4'   value='CY_AUTANALOG_SAR_ACC_CNT4'   visible='true'/>
      <Entry name='8'   value='CY_AUTANALOG_SAR_ACC_CNT8'   visible='true'/>
      <Entry name='16'  value='CY_AUTANALOG_SAR_ACC_CNT16'  visible='true'/>
      <Entry name='32'  value='CY_AUTANALOG_SAR_ACC_CNT32'  visible='true'/>
      <Entry name='64'  value='CY_AUTANALOG_SAR_ACC_CNT64'  visible='true'/>
      <Entry name='128' value='CY_AUTANALOG_SAR_ACC_CNT128' visible='true'/>
      <Entry name='256' value='CY_AUTANALOG_SAR_ACC_CNT256' visible='true'/>
    </ParamChoice>
