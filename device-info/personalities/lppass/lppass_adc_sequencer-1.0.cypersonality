    <ParamChoice id='calibrationInRuntime'
                 name='Calibration in Runtime'
                 group='`${rootGroupName}`'
                 visible='false'
                 editable='true'
                 desc='A real-time calibration channel request'
                                       default='CY_AUTANALOG_SAR_CAL_DISABLED'>
      <Entry name='Disabled'             value='CY_AUTANALOG_SAR_CAL_DISABLED'  visible='true'/>
      <Entry name='Offset and Linearity' value='CY_AUTANALOG_SAR_CAL_BOTH'      visible='true'/>
      <Entry name='Offset Only'          value='CY_AUTANALOG_SAR_CAL_OFFSET'    visible='true'/>
      <Entry name='Linearity Only'       value='CY_AUTANALOG_SAR_CAL_LINEARITY' visible='true'/>
    </ParamChoice>

    <ParamString id='nonSimultaneousSamplingGPIOch' name='nonSimultaneousSamplingGPIOch' group='Internal' visible='`${debug}`' desc='' editable='false'
        default='`${(channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO0) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO0" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO1) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO1" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO2) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO2" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO3) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO3" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO4) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO4" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO5) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO5" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO6) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO6" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO7" :
                     "CY_AUTANALOG_SAR_CHAN_MASK_GPIO_DISABLED"}`' />

    <ParamString id='simultaneousSamplingGPIOch' name='simultaneousSamplingGPIOch' group='Internal' visible='`${debug}`' desc='' editable='false'
        default='`${" &#13; " .
                  (       !(channelGpio0 || channelGpio1 || channelGpio2 || channelGpio3 || channelGpio4 || channelGpio5 || channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO_DISABLED"  : "") .
                  (channelGpio0 &amp;&amp; (channelGpio1 || channelGpio2 || channelGpio3 || channelGpio4 || channelGpio5 || channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO0 | &#13; " : channelGpio0 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO0" : "") .
                  (channelGpio1 &amp;&amp; (                channelGpio2 || channelGpio3 || channelGpio4 || channelGpio5 || channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO1 | &#13; " : channelGpio1 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO1" : "") .
                  (channelGpio2 &amp;&amp; (                                channelGpio3 || channelGpio4 || channelGpio5 || channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO2 | &#13; " : channelGpio2 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO2" : "") .
                  (channelGpio3 &amp;&amp; (                                                channelGpio4 || channelGpio5 || channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO3 | &#13; " : channelGpio3 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO3" : "") .
                  (channelGpio4 &amp;&amp; (                                                                channelGpio5 || channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO4 | &#13; " : channelGpio4 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO4" : "") .
                  (channelGpio5 &amp;&amp; (                                                                                channelGpio6 || channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO5 | &#13; " : channelGpio5 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO5" : "") .
                  (channelGpio6 &amp;&amp; (                                                                                                channelGpio7) ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO6 | &#13; " : channelGpio6 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO6" : "") .
                  (channelGpio7 ? "CY_AUTANALOG_SAR_CHAN_MASK_GPIO7" : "")}`' />

    <ParamString id='nonSimultaneousMuxMode' name='nonSimultaneousMuxMode' group='Internal' visible='`${debug}`' desc='' editable='false'
        default='`${(channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX0)  &amp;&amp; (muxchan_0_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX1)  &amp;&amp; (muxchan_1_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX2)  &amp;&amp; (muxchan_2_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX3)  &amp;&amp; (muxchan_3_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX4)  &amp;&amp; (muxchan_4_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX5)  &amp;&amp; (muxchan_5_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX6)  &amp;&amp; (muxchan_6_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX7)  &amp;&amp; (muxchan_7_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX8)  &amp;&amp; (muxchan_8_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX9)  &amp;&amp; (muxchan_9_pseudoDiffMode)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX10) &amp;&amp; (muxchan_10_pseudoDiffMode) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX11) &amp;&amp; (muxchan_11_pseudoDiffMode) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX12) &amp;&amp; (muxchan_12_pseudoDiffMode) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX13) &amp;&amp; (muxchan_13_pseudoDiffMode) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX14) &amp;&amp; (muxchan_14_pseudoDiffMode) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX15) &amp;&amp; (muxchan_15_pseudoDiffMode) ?
                    "CY_AUTANALOG_SAR_CHAN_CFG_MUX0_PSEUDO_DIFF" :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX0)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX1)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX2)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX3)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX4)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX5)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX6)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX7)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX8)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX9)  ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX10) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX11) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX12) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX13) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX14) ||
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX15) ?
                     "CY_AUTANALOG_SAR_CHAN_CFG_MUX0_SINGLE_ENDED" :
                     "CY_AUTANALOG_SAR_CHAN_CFG_MUX_DISABLED"}`' />

    <ParamString id='simultaneousMuxMode' name='simultaneousMuxMode' group='Internal' visible='`${debug}`' desc='' editable='false'
        default='`${channelMux0 &amp;&amp;
                    ((sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX0)  &amp;&amp; (muxchan_0_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX1)  &amp;&amp; (muxchan_1_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX2)  &amp;&amp; (muxchan_2_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX3)  &amp;&amp; (muxchan_3_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX4)  &amp;&amp; (muxchan_4_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX5)  &amp;&amp; (muxchan_5_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX6)  &amp;&amp; (muxchan_6_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX7)  &amp;&amp; (muxchan_7_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX8)  &amp;&amp; (muxchan_8_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX9)  &amp;&amp; (muxchan_9_pseudoDiffMode)  ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX10) &amp;&amp; (muxchan_10_pseudoDiffMode) ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX11) &amp;&amp; (muxchan_11_pseudoDiffMode) ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX12) &amp;&amp; (muxchan_12_pseudoDiffMode) ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX13) &amp;&amp; (muxchan_13_pseudoDiffMode) ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX14) &amp;&amp; (muxchan_14_pseudoDiffMode) ||
                     (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX15) &amp;&amp; (muxchan_15_pseudoDiffMode)) ?
                      "CY_AUTANALOG_SAR_CHAN_CFG_MUX0_PSEUDO_DIFF" :
                    ( channelMux0 &amp;&amp; !channelMux1) ? "CY_AUTANALOG_SAR_CHAN_CFG_MUX0_SINGLE_ENDED" :
                    (!channelMux0 &amp;&amp;  channelMux1) ? "CY_AUTANALOG_SAR_CHAN_CFG_MUX1_SINGLE_ENDED" :
                    ( channelMux0 &amp;&amp;  channelMux1) ? "CY_AUTANALOG_SAR_CHAN_CFG_MUX0_MUX1_SINGLE_ENDED" :
                      "CY_AUTANALOG_SAR_CHAN_CFG_MUX_DISABLED"}`' />

    <ParamString id='nonSimultaneousMux0Sel' name='nonSimultaneousMux0Sel' group='Internal' visible='`${debug}`' desc='' editable='false'
        default='`${(channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX0)  ? "CY_AUTANALOG_SAR_CHAN_CFG_MUX0"  :
